{
  "name": "crown-commerce-web",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve origin-hair-collective",
    "start:mane-haus": "ng serve mane-haus-coming-soon",
    "build": "ng build",
    "watch": "ng build --watch --configuration development",
    "test": "ng test",
    "storybook": "ng run components:storybook",
    "build-storybook": "ng run components:build-storybook",
    "e2e": "npx playwright test",
    "e2e:headed": "npx playwright test --headed",
    "e2e:ui": "npx playwright test --ui",
    "e2e:chromium": "npx playwright test --project=chromium",
    "e2e:mobile": "npx playwright test --project=mobile-chrome --project=mobile-safari",
    "e2e:admin": "npx playwright test --config=playwright.admin.config.ts",
    "e2e:admin:headed": "npx playwright test --config=playwright.admin.config.ts --headed",
    "e2e:admin:ui": "npx playwright test --config=playwright.admin.config.ts --ui",
    "e2e:admin:chromium": "npx playwright test --config=playwright.admin.config.ts --project=chromium",
    "e2e:coming-soon": "npx playwright test --config=playwright.coming-soon.config.ts",
    "e2e:coming-soon:headed": "npx playwright test --config=playwright.coming-soon.config.ts --headed",
    "e2e:coming-soon:ui": "npx playwright test --config=playwright.coming-soon.config.ts --ui",
    "e2e:coming-soon:chromium": "npx playwright test --config=playwright.coming-soon.config.ts --project=chromium",
    "e2e:teams": "npx playwright test --config=playwright.teams.config.ts",
    "e2e:teams:headed": "npx playwright test --config=playwright.teams.config.ts --headed",
    "e2e:teams:ui": "npx playwright test --config=playwright.teams.config.ts --ui",
    "e2e:teams:chromium": "npx playwright test --config=playwright.teams.config.ts --project=chromium"
  },
  "prettier": {
    "printWidth": 100,
    "singleQuote": true,
    "overrides": [
      {
        "files": "*.html",
        "options": {
          "parser": "angular"
        }
      }
    ]
  },
  "private": true,
  "packageManager": "npm@10.9.4",
  "dependencies": {
    "@angular/animations": "^21.1.3",
    "@angular/cdk": "^21.1.3",
    "@angular/common": "^21.1.0",
    "@angular/compiler": "^21.1.0",
    "@angular/core": "^21.1.0",
    "@angular/forms": "^21.1.0",
    "@angular/material": "^21.1.3",
    "@angular/platform-browser": "^21.1.0",
    "@angular/router": "^21.1.0",
    "@microsoft/signalr": "^10.0.0",
    "rxjs": "~7.8.0",
    "tslib": "^2.3.0"
  },
  "devDependencies": {
    "@angular-devkit/architect": "^0.2101.0",
    "@angular-devkit/build-angular": "^21.1.0",
    "@angular-devkit/core": "^21.1.0",
    "@angular/build": "^21.1.3",
    "@angular/cli": "^21.1.0",
    "@angular/compiler-cli": "^21.1.0",
    "@angular/platform-browser-dynamic": "^21.1.0",
    "@compodoc/compodoc": "^1.2.1",
    "@playwright/test": "^1.58.2",
    "@storybook/addon-a11y": "^10.2.8",
    "@storybook/addon-docs": "^10.2.8",
    "@storybook/addon-onboarding": "^10.2.8",
    "@storybook/angular": "^10.2.8",
    "jsdom": "^27.1.0",
    "ng-packagr": "^21.1.0",
    "storybook": "^10.2.8",
    "typescript": "~5.9.2",
    "vitest": "^4.0.8"
  }
}
